//JLINK PARA VINCULAR O JDK
jlink --module-path "$env:JAVA_HOME\jmods" --add-modules java.base --output dist\jre --compress=2 --no-header-files --no-man-pages


//JPACKAGE PARA TRANSFORMAR O JAR EM UM EXECUTAVEL PORTATIL COM JDK
jpackage --name CliApp --input target --main-jar cli-app-1.0-SNAPSHOT.jar --main-class com.example.cli.CliApp --type app-image --dest dist --app-version 1.0 --win-console --runtime-image dist\jre
